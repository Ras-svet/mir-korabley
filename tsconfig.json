{
  "compilerOptions": {
    "baseUrl": "./src",
    "paths": {
      "@types/*": ["@types/*"]
    },
    "target": "es6",
    "lib": ["dom", "esnext"],
    "jsx": "react-jsx",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "include": [
    "src/**/*",
    "@types/**/*",
  ]
}
